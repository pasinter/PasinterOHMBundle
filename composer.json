{
  "name": "pasinter/ohm-bundle",
  "type": "library",
  "description": "Symfony Bundle for Redis Object-Hash Mapper",
  "keywords": ["persistence", "ohm", "redis", "symfony"],
  "homepage": "https://github.com/pasinter/PasinterOHMBundle",
  "license": "MIT",
  "authors": [
    {"name": "pasinter"}
  ],
  "require": {
    "php": ">=5.4",
    "pasinter/redis-ohm": "dev-master",
    "symfony/framework-bundle": "~2.3|~3.0",
    "symfony/console": "~2.3|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "suggest": {
    "symfony/web-profiler-bundle": "to use the data collector"
  },
  "autoload": {
    "psr-4": { "Pasinter\\Bundle\\OHMBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
